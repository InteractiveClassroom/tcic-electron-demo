{
  "name": "tcic-electron",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "build:mac": "electron-builder --mac",
    "build:win": "electron-builder --win"
  },
  "build": {
    "productName": "tcic-demo",
    "appId": "com.tencent.ilivesdk-tcic",
    "extraResources": [
      {
        "from": "./.profile",
        "to": "./"
      },
      {
        "from": "./img",
        "to": "./img"
      }
    ],
    "asarUnpack": [
      "**/*.{node,dll}",
      "**/node_modules/**/tcic-electron-sdk/dist/assets/**/*",
      "**/node_modules/**/trtc-electron-sdk/build/**/*"
    ],
    "win": {
      "target": ["nsis"],
      "extraFiles": [
        {
          "from": "node_modules/trtc-electron-sdk/build/Release/",
          "to": "./resources",
          "filter": [
            "**/*"
          ]
        },
        {
          "from": "node_modules/trtc-electron-plugin-xmagic/plugin/XMagic/win/${arch}/platforms/",
          "to": "./platforms",
          "filter": [
            "**/*"
          ]
        },
        {
          "from": "node_modules/trtc-electron-plugin-xmagic/plugin/XMagic/win/${arch}/",
          "to": "./resources/app/plugin/XMagic/win/${arch}/",
          "filter": [
            "**/*"
          ]
        },
        {
          "from": "node_modules/trtc-electron-plugin-xmagic/plugin/XMagic/win/res/",
          "to": "./resources/app/plugin/XMagic/win/res/",
          "filter": [
            "**/*"
          ]
        }
      ]
    },
    "mac": {
      "target": "dmg",
      "extraFiles": [
        {
          "from": "node_modules/trtc-electron-sdk/build/Release/${arch}/trtc_electron_sdk.node",
          "to": "./"
        },
        {
          "from": "node_modules/trtc-electron-sdk/build/mac-framework/${arch}/",
          "to": "./Frameworks"
        }
      ]
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "lcic-electron-hotupdater": "^0.1.0-latest.6",
    "trtc-electron-plugin-xmagic": "^1.0.2",
    "tcic-electron-sdk": "^1.10.0-beta.250916.1206.25",
    "trtc-electron-sdk": "^12.5.705-beta.0"
  },
  "devDependencies": {
    "electron": "24.8.8",
    "electron-builder": "^25.1.8"
  }
}
